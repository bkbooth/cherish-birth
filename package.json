{
  "name": "cherish-birth",
  "version": "1.4.1",
  "description": "Cherish Birth website",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "version": "npm run buildcss && npm run buildjs && git add --all public/",
    "buildcss": "cleancss --source-map -r public/css/ -o public/css/style.min.css public/css/style.css",
    "buildjs": "uglifyjs public/js/script.js -c -m -p 2 -o public/js/script.min.js --source-map public/js/script.min.js.map --source-map-url script.min.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/bkbooth/cherish-birth.git"
  },
  "author": "Ben Booth <bkbooth@gmail.com>",
  "license": "ISC",
  "homepage": "https://cherishbirth.com.au",
  "dependencies": {
    "dotenv": "2.0.0",
    "express": "4.13.4",
    "express-hbs": "1.0.1",
    "md5-file": "3.1.0",
    "morgan": "1.7.0",
    "nodemon": "1.9.2"
  },
  "devDependencies": {
    "clean-css": "3.4.17",
    "uglify-js": "2.6.2"
  }
}
