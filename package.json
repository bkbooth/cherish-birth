{
  "name": "cherish-birth",
  "version": "2.0.0",
  "description": "Cherish Birth website",
  "private": true,
  "scripts": {
    "gulp": "gulp",
    "start": "npm-run-all --parallel watch serve",
    "build": "NODE_ENV=production npm-run-all clean gulp",
    "clean": "gulp clean",
    "watch": "gulp watch",
    "serve": "http-server dist -a 0.0.0.0 -p 8000",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build && echo 'cherishbirth.com.au' > dist/CNAME"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkbooth/cherish-birth.git"
  },
  "author": "Ben Booth <bkbooth@gmail.com>",
  "license": "ISC",
  "homepage": "https://cherishbirth.com.au",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "cssnano": "^3.10.0",
    "del": "^2.2.2",
    "gh-pages": "^0.12.0",
    "gulp": "^3.9.1",
    "gulp-cache-bust": "^1.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.1.2",
    "handlebars": "^4.0.6",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2"
  }
}
